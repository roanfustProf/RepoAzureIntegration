{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nombre de instancia de Data Factory",
			"defaultValue": "adfIntegrationAzureSqlADFTest"
		},
		"lsazureblobstorage_connectionString": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"connectionString\"de \"lsazureblobstorage\""
		},
		"lazurekv_desa_per_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kvdesaper.vault.azure.net/"
		},
		"lsrcDestination_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "@linkedService().ParamDataSourceCode"
		},
		"default_properties_EnvironmentADF_value": {
			"type": "string",
			"defaultValue": "DESA_PER"
		},
		"default_properties_CountryCodeADF_value": {
			"type": "string",
			"defaultValue": "PE"
		},
		"default_properties_Environment_value": {
			"type": "string",
			"defaultValue": "DESA"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI de la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas."
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Token de SAS para la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfIntegrationAzureSqlADFTest_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"lsazureblobstorage_connectionString": {
						"value": "[parameters('lsazureblobstorage_connectionString')]"
					},
					"default_properties_EnvironmentADF_value": {
						"value": "[parameters('default_properties_EnvironmentADF_value')]"
					},
					"default_properties_CountryCodeADF_value": {
						"value": "[parameters('default_properties_CountryCodeADF_value')]"
					},
					"default_properties_Environment_value": {
						"value": "[parameters('default_properties_Environment_value')]"
					},
					"lazurekv_desa_per_properties_typeProperties_baseUrl": {
						"value": "[parameters('lazurekv_desa_per_properties_typeProperties_baseUrl')]"
					},
					"lsrcDestination_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('lsrcDestination_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adfIntegrationAzureSqlADFTest_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adfIntegrationAzureSqlADFTest_ArmTemplate_0')]"
			]
		}
	]
}